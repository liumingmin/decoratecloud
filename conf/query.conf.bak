<QueryInfo>
	<Id>query_1001</Id>
	<RefModelNames>Project,ProjectUserLink,SysUser</RefModelNames>
	<SQL>select p.Id,p.Name,u.RealName ProjCreateUser,p.PictureId from projectuserlink pu,project p
         left join sysuser u on p.CreateUserId=u.Id
         where p.Id=pu.ProjectId and pu.IsOwner = 0 %s
         order by p.Name
	</SQL>

	<BindParams>
        <BindParamGroup ConnSymbol="and">
        			<BindParam ConnSymbol="and">
        				<FieldExpress> pu.UserId ='%s' </FieldExpress>
        				<FormName>UserId</FormName>
        			</BindParam>
        		</BindParamGroup>
	</BindParams>
	<Remark>
项目页面
	</Remark>
</QueryInfo>


<QueryInfo>
	<Id>query_1003</Id>
	<RefModelNames>Project,ProjectJoinRequest,SysUser</RefModelNames>
	<SQL>
	select r.id,r.reqkind,r.Result,r.Msg,p.Name,r.JoinUserId,u.RealName,u2.RealName,r.ProjectId,u.PictureId,u2.PictureId,u.Id,u2.Id from projectjoinrequest r
    left join project p on r.ProjectId = p.Id
    left join sysuser u on r.ProjectUserId=u.Id
    left join sysuser u2 on r.JoinUserId=u2.Id
    where  r.Result =0  %s
    order by r.requestTime desc
	</SQL>

	<BindParams>
        <BindParamGroup ConnSymbol="and">
        			<BindParam ConnSymbol="or">
                        <FieldExpress>  (r.JoinUserId ='%s' and r.ReqKind=1) </FieldExpress>
                        <FormName>UserId</FormName>
                    </BindParam>
                    <BindParam ConnSymbol="or">
                        <FieldExpress>  (r.ProjectUserId ='%s' and r.ReqKind=0) </FieldExpress>
                        <FormName>UserId</FormName>
                    </BindParam>
        </BindParamGroup>

	</BindParams>
	<Remark>
项目页面
	</Remark>
</QueryInfo>

<QueryInfo>

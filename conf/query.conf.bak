	<RefModelNames>ProjectUserLink,SysUser</RefModelNames>
	<SQL>(select distinct u.id,u.UserName,u.RealName,u.PictureId,'1' kind from projectuserlink pu, sysuser u
                  where pu.UserId=u.Id and pu.IsOwner = 0 %s
                  order by u.RealName)
          union all

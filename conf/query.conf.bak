<QueryInfo>
	<Id>query_1007</Id>
	<RefModelNames>SysUser,ProjectUserLink,ProjectUserTag</RefModelNames>
	<SQL>

select  u.id,u.UserName,u.RealName,u.PictureId,group_concat(g.Tag) tags from  sysuser u,projectuserlink pu
    left  join projectusertag g on (pu.ProjectId = g.ProjectId and pu.UserId=g.UserId)
                          where pu.UserId=u.Id  %s
                          group by u.id,u.UserName,u.RealName,u.PictureId
                          order by u.RealName
	</SQL>
    <BindParams>
        <BindParamGroup ConnSymbol="and">
        			<BindParam ConnSymbol="and">
        				<FieldExpress>  pu.ProjectId ='%s' </FieldExpress>
        				<FormName>ProjectId</FormName>
        			</BindParam>
        			<BindParam ConnSymbol="and">
                        <FieldExpress>  pu.UserId &lt;&gt;'%s' </FieldExpress>
                        <FormName>UserId</FormName>
                    </BindParam>
        </BindParamGroup>
	</BindParams>

	<Remark>
全局当前项目 选择用户
	</Remark>
</QueryInfo>

